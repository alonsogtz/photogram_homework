<div class="page-header">
  <h3>Posts Like #<%= @posts_like.id %></h3>
</div>

<div class="row">
  <div class="col-md-12 mb-2">
    <dl class="dl-horizontal">
      <dt>User</dt>
      <dd><%= @posts_like.user_id %></dd>

      <dt>Post</dt>
      <dd><%= @posts_like.post_id %></dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/posts_likes" class="btn btn-primary">
        Back
      </a>
      <a href="/posts_likes/<%= @posts_like.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_posts_like/<%= @posts_like.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>
